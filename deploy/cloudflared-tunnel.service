[Unit]
Description=Cloudflare Tunnel for VokaFlow Backend
After=network.target

[Service]
Type=simple
User=dw7
WorkingDirectory=/home/dw7
ExecStart=/usr/bin/cloudflared tunnel --config /home/dw7/.cloudflared/config.yml run
Restart=on-failure
RestartSec=5
TimeoutStartSec=0

[Install]
WantedBy=multi-user.target 